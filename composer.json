{
    "name": "yiisoft/yii-event",
    "type": "library",
    "description": "Yii Event",
    "keywords": [
        "yii3",
        "event"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-event/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-event"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/friendly-exception": "^1.0",
        "yiisoft/injector": "^1.0",
        "yiisoft/event-dispatcher": "^3.0@dev"
    },
    "require-dev": {
        "infection/infection": "^0.18.0",
        "phpunit/phpunit": "^9.4",
        "vimeo/psalm": "^3.18 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Event\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\Event\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
